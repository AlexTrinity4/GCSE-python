import random
num = random.randint(1,5)
guess = int(input("Guess a number"))
if guess == num:
    print("Well done!")
else:
    if guess > num:
        print("Too high")
    if guess < num:
        print("Too low")
guess2 = int(input("Try again"))
if guess2 == num:
    print("Correct!")
else:
    print("You lose")
